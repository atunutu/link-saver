{
    "manifest_version": 3,
    "name": "Link Saver",
    "version": "1.0",
    "description": "Save and manage links for online applications i.e linkedIn links, portfolio links, github etc ",
    "permissions": [
      "storage",           
      "contextMenus"        
    ],
    "background": {
      "service_worker": "background.js"  
    },
    "action": {
      "default_popup": "popup.html",     
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  